{
  "name": "Sails Color Theme Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#001c20",
    "editorCursor.foreground": "#14acc2",
    "editor.lineHighlightBackground": "#e3f0f1",
    "editorLineNumber.foreground": "#2e484f",
    "editor.selectionBackground": "#0c8da044",
    "sideBar.background": "#f5f5f5",
    "sideBar.foreground": "#001c20",
    "activityBar.background": "#f5f5f5",
    "statusBar.background": "#f5f5f5",
    "statusBar.foreground": "#001c20",
    "terminal.background": "#ffffff",
    "terminal.foreground": "#001c20"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e8a91",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#14acc2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string", "constant.character.escape"],
      "settings": {
        "foreground": "#0c8da0"
      }
    },
    {
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#006d7d"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#001c20"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#14acc2"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class"],
      "settings": {
        "foreground": "#001c20"
      }
    }
  ]
} 